#!/usr/bin/env perl6

use v6;

my Bool $found-data = False;
my @constants;

for 'lib/Net/LDAP/Constant.pm'.IO.lines -> $line {
    $line.chomp;
    $found-data = True if $line ~~ /^__DATA__$/;
    next unless $found-data;
    last if $line ~~ /^\=cut/;
    # next unless $line ~~ /^\=item\s+(LDAP_\S+)\s+((.*)\\)/;
    next unless $line ~~ /^\=item\s+(LDAP_\S+)/;
    say $0, ", ", $1;
#    my $protocol-const = :  
#     say $line;
}
